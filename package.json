{
    // Project metadata
    "name": "fraudprevention",
    "version": "1.0.0",
    "description": "FraudPrevention - A TypeScript implementation",
    "main": "dist/index.js",
    "author": "mpermar082",
    "license": "MIT",
    
    // Script commands
    "scripts": {
        // Build command: Compile TypeScript code
        "build": "tsc",
        // Start command: Run compiled JavaScript code
        "start": "node dist/index.js",
        // Dev command: Run TypeScript code with ts-node
        "dev": "ts-node src/index.ts",
        // Test command: Run Jest tests
        "test": "jest",
        // Clean command: Remove compiled JavaScript code
        "clean": "rm -rf dist"
    },
    
    // Project dependencies
    "dependencies": {
        "minimist": "^1.2.0" // Dependency for command-line argument parsing
    },
    
    // Project development dependencies
    "devDependencies": {
        "typescript": "^5.0.0", // TypeScript compiler
        "@types/node": "^18.0.0", // TypeScript type definitions for Node.js
        "@types/minimist": "^1.2.0", // TypeScript type definitions for minimist
        "ts-node": "^10.0.0", // TypeScript execution environment
        "jest": "^29.0.0", // Testing framework
        "@types/jest": "^29.0.0" // TypeScript type definitions for Jest
    }
}